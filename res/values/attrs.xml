<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Speedometer">
        <attr name="maxSpeed" format="float"></attr>
        <attr name="currentSpeed" format="float"></attr>
        <attr name="onColor" format="color"></attr>
        <attr name="offColor" format="color"></attr>
        <attr name="scaleColor" format="color"></attr>
        <attr name="scaleTextSize" format="dimension"></attr>
        <attr name="readingTextSize" format="dimension"></attr>
    </declare-styleable>
    
    
  
    
    <declare-styleable name="RoundProgressBar">  
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        
        <attr name="textColor" format="color" />  
        <attr name="textSize" format="dimension" /> 
        <attr name="max" format="integer"></attr> 
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
        <attr name="keepRoundType">
            <enum name="NOT_KEEP" value="0"></enum>
            <enum name="KEEP" value="1"></enum>
        </attr>
    </declare-styleable> 
   <declare-styleable name="RoundProgressBar2">  
        <attr name="roundColor1" format="color"/>
        <attr name="roundProgressColor1" format="color"/>
        <attr name="roundWidth1" format="dimension"></attr>
        
        <attr name="textColor1" format="color" />  
        <attr name="textSize1" format="dimension" /> 
        <attr name="max1" format="integer"></attr> 
        <attr name="textIsDisplayable1" format="boolean"></attr>
        <attr name="style1">
            <enum name="STROKE1" value="0"></enum>
            <enum name="FILL1" value="1"></enum>
        </attr>
        <attr name="keepRoundType1">
            <enum name="NOT_KEEP1" value="0"></enum>
            <enum name="KEEP1" value="1"></enum>
        </attr>
    </declare-styleable> 
</resources>